{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {},
  "id": "http://example.com/example.json",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "district": {
          "title": "District",
          "type": "string"
        },
        "details": {
          "id": "/properties/data/properties/questions",
          "title": "District Details",
            "properties": {
              "district_area": {
                "title": "Total Area",
                "id": "/properties/data/properties/questions/items/properties/question",
                "type": "string",
                "default": "None"
              },
              "district_population": {
                "type": "string",
                "title": "Total Population",
                "default": ""
              },
              "district_map": {
                "type": "string",
                "title": "Map Image",
                "default": ""
              },
              "urban_population": {
                "type": "string",
                "title": "Urban Population",
                "default": "50"
              },
              "rural_population": {
                "type": "string",
                "title": "Rural Population",
                "default": "50"
              },
              "males": {
                "type": "string",
                "title": "Male Percentage",
                "default": "50"
              },
              "females": {
                "type": "string",
                "title": "Female Percentage",
                "default": "50"
              },
              "district_info": {
                "type": "string",
                "title": "Information",
                "default": ""
              }
            },
            "required": [
              "district_area",
              "district_population",
              "urban_population",
              "rural_population",
              "males",
              "females",
              "district_map",
              "district_info"
            ],
            "minItems": 1,
            "type": "object"
          }
      }
    }
  },
  "type": "object"
}
